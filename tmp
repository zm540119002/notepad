$domain_arr = getSingleDomainByArr($all_arr);
    $all_array1 = array();
    $checked_arr = array();
    $a = 0;
    $i = 0;
    foreach($domain_arr as $kkk=>$vvv){
        $b = 0;
        $array2 = array();
        $service_arr = getSingleServiceByArr($all_arr,$vvv[0]['CODE']) ;
        foreach($service_arr as $kk=>$vv){
            $c = 0;
            $array1 = array();
            $sub_service_arr = getSingleSubServiceByArr($all_arr,$vv[0]['SERVICE_ID']) ;
            foreach($sub_service_arr as $k=>$v){
                if($v[1]){
                    $checked_arr[$i] = $v[0]['SUB_SERVICE_ID'];
                    $i++;
                }
                $array1[$c]= array(
                    "id" => $v[0]['SUB_SERVICE_ID'],
                    "label" => $v[0]['SUB_SERVICE_NAME']
                );
                $c++;
            }
            /*print($bool);exit;*/
            $array2[$b] = array(
                "id" => $vv[0]['SERVICE_ID'],
                "label" => $vv[0]['SERVICE_NAME'],
                "children" => $array1
            );
            $b++;
        }
        $array3[$a] = array(
            "id" => $vvv[0]['CODE'],
            "label" => $vvv[0]['NAME_CN'],
            "children" => $array2
        );
        $a++;
    }
    $config['all_array1'] = $array3;
    $config['checked_arr'] = $checked_arr;
    $config = json_encode($config);
    echo $config;
