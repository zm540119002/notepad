$domain_arr = getSingleDomainByArr($all_arr) ;
    $all_array1 = array();
    $checked_arr = array();
    $a = 0;
    $i = 0;
    foreach($domain_arr as $kkkk=>$vvvv){
        $b = 0;
        $array2 = array();
        $service_arr = getSingleServiceByArr($all_arr,$vvvv[0]['CODE']) ;
        foreach($service_arr as $kkk=>$vvv){
            $c = 0;
            $array3 = array();
            $sub_service_arr = getSingleSubServiceByArr($all_arr,$vvv[0]['SERVICE_ID']) ;
            foreach($sub_service_arr as $kk=>$vv){
                $d = 0;
                $array4 = array();
                $rpt_arr = getSingeRptByArr($all_arr,$vv[0]['SUB_SERVICE_ID']) ;
                foreach($rpt_arr as $k=>$v){
                    if($v[1]){
                        $checked_arr[$i] = $v[0]['REPORT_ID'];
                        $i++;
                    }
                    $array4[$d]= array(
                        "id" => $v[0]['REPORT_ID'],
                        "label" => $v[0]['NAME']
                    );
                    $d++;
                }
                $array3[$c]= array(
                    "id" => $vv[0]['SUB_SERVICE_ID'],
                    "label" => $vv[0]['SUB_SERVICE_NAME'],
                    "children" => $array4
                );
                $c++;
            }
            $array2[$b]= array(
                "id" => $vvv[0]['SERVICE_ID'],
                "label" => $vvv[0]['SERVICE_NAME'],
                "children" => $array3
            );
            $b++;
        }
        $array1[$a]= array(
            "id" => $vvvv[0]['CODE'],
            "label" => $vvvv[0]['NAME_CN'],
            "children" => $array2
        );
        $a++;
    }
    $config['all_array1'] = $array1;
    $config['checked_arr'] = $checked_arr;
    $config =  json_encode($config);