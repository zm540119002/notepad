drop table ua_dbg.exponent_smooth_param 
--1.	指数平滑模型表
create table ua_dbg.exponent_smooth_param
(
  serial_id number PRIMARY KEY not null,--序号
  algorithm_task_id number not null,--算法任务id
  data_cycle VARCHAR2(64) not null,--数据周期
  data_value  number not null,--数据值
  is_recommend       char(2) default '1' not null,--是否自动推荐
  
  algorithm_name VARCHAR2(64) not null,--算法名称
  smooth_level  number not null,--平滑系数
  sensitivity  number not null--灵敏度
);
comment on table ua_dbg.exponent_smooth_param is '指数平滑模型表';
comment on column ua_dbg.exponent_smooth_param.serial_id is '序号';
comment on column ua_dbg.exponent_smooth_param.algorithm_task_id is '算法任务id';
comment on column ua_dbg.exponent_smooth_param.data_cycle is '数据周期';
comment on column ua_dbg.exponent_smooth_param.data_value is '数据值';
comment on column ua_dbg.exponent_smooth_param.is_recommend is '是否自动推荐';
comment on column ua_dbg.exponent_smooth_param.algorithm_name is '算法名称';
comment on column ua_dbg.exponent_smooth_param.smooth_level is '平滑系数';
comment on column ua_dbg.exponent_smooth_param.sensitivity is '灵敏度';

--2.	Holt Winter模型
drop table ua_dbg.holt_winter_param
create table ua_dbg.holt_winter_param
(
  serial_id number PRIMARY KEY not null,--序号
  algorithm_task_id number not null,--算法任务id
  data_cycle VARCHAR2(64) not null,--数据周期
  data_value  number not null,--数据值
  is_recommend       char(2) default '1' not null,--是否自动推荐
  
  seasonal_cycle VARCHAR2(64) not null,--季节性周期
  trend_increase  number not null,--趋势增长方式
  seasonal_increase  number not null,--季节性增长方式
  verification_number number not null,--验证集个数
  sensitivity  number not null--灵敏度
);
comment on table ua_dbg.holt_winter_param is 'Holt Winter模型表';
comment on column ua_dbg.holt_winter_param.serial_id is '序号';
comment on column ua_dbg.holt_winter_param.algorithm_task_id is '算法任务id';
comment on column ua_dbg.holt_winter_param.data_cycle is '数据周期';
comment on column ua_dbg.holt_winter_param.data_value is '数据值';
comment on column ua_dbg.holt_winter_param.is_recommend is '是否自动推荐';
comment on column ua_dbg.holt_winter_param.seasonal_cycle is '季节性周期';
comment on column ua_dbg.holt_winter_param.trend_increase is '趋势增长方式';
comment on column ua_dbg.holt_winter_param.seasonal_increase is '季节性增长方式';
comment on column ua_dbg.holt_winter_param.verification_number is '验证集个数';
comment on column ua_dbg.holt_winter_param.sensitivity is '灵敏度';

--3.  差分自回归移动平均ARIMA模型
drop table ua_dbg.arima_param
create table ua_dbg.arima_param
(
  serial_id number PRIMARY KEY not null,--序号
  algorithm_task_id number not null,--算法任务id
  data_cycle VARCHAR2(64) not null,--数据周期
  data_value  number not null,--数据值
  is_recommend       char(2) default '1' not null,--是否自动推荐
  
  autoregressive number not null,--自回归阶数
  difference_count  number not null,--差分次数
  translation  number not null,--平移阶数
  verification_number number not null,--验证集个数
  sensitivity  number not null--灵敏度
);
comment on table ua_dbg.arima_param is 'ARIMA模型表';
comment on column ua_dbg.arima_param.serial_id is '序号';
comment on column ua_dbg.arima_param.algorithm_task_id is '算法任务id';
comment on column ua_dbg.arima_param.data_cycle is '数据周期';
comment on column ua_dbg.arima_param.data_value is '数据值';
comment on column ua_dbg.arima_param.is_recommend is '是否自动推荐';
comment on column ua_dbg.arima_param.autoregressive is '自回归阶数';
comment on column ua_dbg.arima_param.difference_count is '差分次数';
comment on column ua_dbg.arima_param.translation is '平移阶数';
comment on column ua_dbg.arima_param.verification_number is '验证集个数';
comment on column ua_dbg.arima_param.sensitivity is '灵敏度';

--4.  循环神经网络RNN模型
drop table ua_dbg.rnn_param
create table ua_dbg.rnn_param
(
  serial_id number PRIMARY KEY not null,--序号
  algorithm_task_id number not null,--算法任务id
  data_cycle VARCHAR2(64) not null,--数据周期
  data_value  number not null,--数据值
  is_recommend       char(2) default '1' not null,--是否自动推荐
  
  time_step number not null,--时间步
  hidden_dimension  number not null,--隐藏层维度
  iteration_count  number not null,--迭代次数
  verification_number number not null,--验证集个数
  sensitivity  number not null--灵敏度
);
comment on table ua_dbg.rnn_param is 'RNN模型表';
comment on column ua_dbg.rnn_param.serial_id is '序号';
comment on column ua_dbg.rnn_param.algorithm_task_id is '算法任务id';
comment on column ua_dbg.rnn_param.data_cycle is '数据周期';
comment on column ua_dbg.rnn_param.data_value is '数据值';
comment on column ua_dbg.rnn_param.is_recommend is '是否自动推荐';
comment on column ua_dbg.rnn_param.time_step is '时间步';
comment on column ua_dbg.rnn_param.hidden_dimension is '隐藏层维度';
comment on column ua_dbg.rnn_param.iteration_count is '迭代次数';
comment on column ua_dbg.rnn_param.verification_number is '验证集个数';
comment on column ua_dbg.rnn_param.sensitivity is '灵敏度';
