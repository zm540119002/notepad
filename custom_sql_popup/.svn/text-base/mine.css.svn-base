@CHARSET "UTF-8";


.hidden {
	display: none;
}

.scroll_div {
	overflow: auto;
}

.fix_div {
	overflow: hidden;	
}

.div_width_percent {
	box-sizing: border-box;
	-moz-box-sizing: border-box; /* Firefox */
	-webkit-box-sizing: border-box; /* Safari */
}

#custom_sql_main {
	width: 950px;
	height: 550px;
	margin: 0px;
	padding: 0px;
}

#custom_sql_upper {
	width: 100%;
	height: 200px;
	margin: 0px;
	padding: 0px;
}

#custom_sql_ds {
	width: 50%;
	height: 200px;
	float: left;
	padding: 0px;	
	margin: 0px;
}

#custom_sql_column {
	position: relative;
	left: 0px;
	width: 50%;
	height: 200px;
	padding: 0px;	
	margin: 0px;		
}

.textList {
	overflow: auto;
	margin-left: 10px;
	margin-right: 10px;
	height: 200px;	
}

#custom_sql_input {
	width: 95%;
	height: 250px;
	padding-top: 20px;	
	margin-left: 10px;
	margin-right: 10px;	
}


.container_clear {
	clear: both;
}

.ul_tab_menu {
	padding: 0px;
	margin: 0px;
	height: 25px;	
}

.ul_tab_menu .normal {
	float: left;
	padding: 0px;
	margin: 0px 10px 0px 0px;
	color: #ffffff;
	height: 24px;
	line-height: 24px;	
	width: 100px;
	overflow: hidden;
	list-style-type: none;
	background: #F06700;
	text-align: center;
	vertical-align: middle;
	outline-width: 1px;
	outline-style: solid;
	outline-color: #F06700;
}

.ul_tab_menu .hover{
	background: #FFDA8A;
	color: #F0F0FF;
}

.ul_tab_menu .selected {
	background: #FFDA8A;
	color: #F06700;	
	font-weight: bold;
}

.div_center{
    margin-left: auto;
	margin-right: auto;
}

.ul_button{
	float: left;
	padding: 0px 2px 2cm;
	list_style_type: none;
}

.sql_input {
	position: relative;
	top: 0px;
	resize: none;
	width: 100%;
	height: 95%;
	overflow-x: auto;
	overflow-y: auto;
	padding: 0px;
	margin: 0px;
}	